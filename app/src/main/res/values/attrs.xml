<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="mColorAccent" format="color"/>
    <attr name="mColorBackground" format="color"/>
    <attr name="mColorOnBackground" format="color"/>
    <attr name="mColorSurface" format="color"/>
    <attr name="mColorOnSurface" format="color"/>
    <attr name="mColorDivider" format="color"/>
    <attr name="mColorOptionGo" format="color"/>
    <attr name="mColorPreview" format="color"/>

    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="circle" value="1"/>
            <enum name="round" value="2"/>
        </attr>
        <attr name="radius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MyToolbar">
<!--        工具栏选项-->
        <attr name="menu" format="reference"/>
<!--        工具栏导航栏图标颜色-->
        <attr name="navigationIconTint" format="color"/>
<!--        工具栏选项图标颜色-->
        <attr name="menuIconTint" format="color"/>
    </declare-styleable>
</resources>